{
    "name": "mailchecker-io/mailchecker-php-sdk",
    "description": "An SDK to easily work with the MailChecker.io API",
    "keywords": [
        "mailchecker",
        "email validation",
        "validate email",
        "api",
        "sdk"
    ],
    "homepage": "https://github.com/mailchecker-io/mailchecker-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Danny Streur",
            "email": "support@mailchecker.io",
            "homepage": "https://mailchecker.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "nesbot/carbon": "^2.35.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "vlucas/phpdotenv": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "MailChecker\\PhpSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MailChecker\\PhpSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
